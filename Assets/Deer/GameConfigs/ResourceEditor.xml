<?xml version="1.0" encoding="UTF-8"?>
<UnityGameFramework>
  <ResourceEditor>
    <Settings>
      <SourceAssetRootPath>Assets/Deer</SourceAssetRootPath>
      <SourceAssetSearchPaths>
        <SourceAssetSearchPath RelativePath="AssetsHotfix" />
        <SourceAssetSearchPath RelativePath="AssetsPkg" />
      </SourceAssetSearchPaths>
      <SourceAssetUnionTypeFilter>
        t:Scene t:Prefab t:Shader t:Model t:Material t:Texture t:AudioClip t:AnimationClip t:AnimatorController t:TextAsset t:ScriptableObject t:asset t:SpriteAtlas t:spriteatlas
      </SourceAssetUnionTypeFilter>
      <SourceAssetUnionLabelFilter>
        l:AssetBundleInclusive
      </SourceAssetUnionLabelFilter>
      <SourceAssetExceptTypeFilter>
        t:Script t:Font t:ttf
      </SourceAssetExceptTypeFilter>
      <SourceAssetExceptLabelFilter>
        l:AssetBundleExclusive
      </SourceAssetExceptLabelFilter>
      <AssetSorter>Path</AssetSorter>
    </Settings>
  </ResourceEditor>
</UnityGameFramework>